    "mtempy": "^1.0.0",
    "ndarray": "^1.0.19",
    "next": "^12.0.7",
    "next-connect": "^1.0.0",
    "node-fetch": "^3.2.3",
    "node-html-to-image": "^3.2.4",
    "nodejs-whisper": "^0.1.6",
    "pexels": "^1.4.0",
    "progress-stream": "^2.0.0",
    "react": "^17.0.2",
    "react-18-image-lightbox": "^5.1.4",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.3",
    "react-grid-gallery": "^1.0.1-alpha.0",
    "react-modal": "^3.16.1",
